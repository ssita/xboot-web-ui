{
  "name": "xboot-web-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main": "cd app-main && npm install",
    "install:react": "cd app-react && npm install",
    "install:vue": "cd app-vue && npm install",
    "start": "npm-run-all --parallel start:*",
    "start:react": "cd app-react && npm serve:dev",
    "start:vue": "cd app-vue && npm serve:dev",
    "start:main": "cd app-main && npm serve:dev"
  },
  "dependencies": {},
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
